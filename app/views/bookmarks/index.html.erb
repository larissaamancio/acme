<!DOCTYPE html>
<html>
	<head>
	<link href="/assets/custom.css?body=1" media="screen" rel="stylesheet">
	<link href="/assets/bootstrap.css?body=1" media="screen" rel="stylesheet">
	</head>
	<body>
		<div class="container">
		
		  <% @list_bookmarks.each do |l|%>
			  <div class="panel-group" id="list-refresh">
			    <div class="panel panel-default">
			      <div class="panel-heading">
			        <h4 class="panel-title">
			          <a data-toggle="collapse" href="#collapse<%=l.id%>", class="accordion_name", data-id="<%=l.id%>"><%= l.list.name%></a>
			        </h4>
			      </div>
			      <div id="collapse<%=l.id%>" data-id="<%=l.id%>", class="panel-collapse collapse">

			         <%= render :partial => "lists/accordion_list", :locals => {:list_id => l.list.id}%>

			      </div>
			    </div>
			  </div>
		  <% end %>
		</div>
	</body>
</html>



